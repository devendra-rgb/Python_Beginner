number={"jyoti":1, "naveen":10, "vyshu":7, "reddy":8, "manideep":9}

# saving 5 people and their values of numbers and printing numbers using keys as their names

print(number)

print(number["jyoti"])

print(number["vyshu"])

print(number["reddy"])

print(number["manideep"])

print(number["naveen"])



