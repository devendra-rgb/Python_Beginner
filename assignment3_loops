


pizza=["cheese","chicken","veggie","american"] # print favourite pizza names using for loop

print (pizza)


for i in range(len(pizza)):
    print(pizza)

print("we love pizza a lot")


animals=["dolphin","shark","fish"] #print sea animals list
for i in range(len(animals)):
    print("We love sea species"+ animals[i].upper())

print ("sea animals are good")


for num in range(1,20): # printing number between 1-20
    print(num)


numbers = list(range(1,1000000))
print(numbers)

for numbers in range(1,1000000):  # printing number in range of 1 to 1 million
    print(numbers)

min_number = min(numbers)
print(min_number)
